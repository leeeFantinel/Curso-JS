var maior = function(a){
	for (var i = 1; i < 5; i++) {
  	var numero = parseInt(prompt("Digite um número"));
  	if (numero > a) {
    	a = numero;
  	}
	}
  return a;
}


var numero01 = parseInt(prompt("Digite um número"));
var maiorNum = maior(numero01);

alert("O maior número é: " + maiorNum);
console.log("O maior número é: " + maiorNum);
